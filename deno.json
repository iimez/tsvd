{
	"imports": {
		"@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.8",
		"@cliffy/ansi": "jsr:@cliffy/ansi@1.0.0-rc.8",
		"@cliffy/prompt": "jsr:@cliffy/prompt@1.0.0-rc.8",
		"@ai-sdk/anthropic": "npm:@ai-sdk/anthropic@2.0.39",
		"@openrouter/ai-sdk-provider": "npm:@openrouter/ai-sdk-provider@1.2.0",
		"ai": "npm:ai@5.0.82"
	},
	"compilerOptions": {
		"lib": ["es2023", "dom", "dom.iterable", "deno.ns"],
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitOverride": true,
		"strictNullChecks": true,
		"noFallthroughCasesInSwitch": true
	},
	"nodeModulesDir": "auto",
	"lock": false,
	"tasks": {
		"check": "deno check **/*.ts",
		"fmt": "deno fmt",
		"lint": "deno lint"
	},
	"fmt": {
		"useTabs": true,
		"indentWidth": 4,
		"semiColons": false,
		"singleQuote": true
	},
	"lint": {
		"rules": {
			"tags": ["recommended"]
		}
	}
}
